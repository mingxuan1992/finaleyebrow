<script>
	var index = 0;
	var width = 240;

	document.getElementById('leftbutton').addEventListener('click', function() {
		index = index - 1;
		if (index < 0) {
			index = 0;
		}
		dotransition();
	});
	document.getElementById('rightbutton').addEventListener('click',
			function() {
				index = index + 1;
				if (index > 4) {
					index = 4;
				}
				dotransition();
			});

	function dotransition() {
		document.getElementById('wrap-list').style.cssText = "transform: translate3d(-"
				+ index
				* width
				+ "px, 0, 0);	transition-delay: .3s;transition: ease; transition-duration:0.5s;";

	}

	function drawLeft() {
		var c = document.getElementById("leftCanvas"), cxt = c.getContext("2d");
		cxt.beginPath();
		cxt.moveTo(8, 2);
		cxt.lineTo(2, 20);
		cxt.lineTo(8, 38);
		cxt.lineWidth = 4;
		cxt.strokeStyle = "black";
		cxt.lineCap = 'round';
		cxt.lineJoin = "round";
		cxt.stroke();
	}
	drawLeft();

	function drawRight() {
		var c = document.getElementById("rightCanvas"), cxt = c
				.getContext("2d");
		cxt.beginPath();
		cxt.moveTo(2, 2);
		cxt.lineTo(8, 20);
		cxt.lineTo(2, 38);
		cxt.lineWidth = 4;
		cxt.strokeStyle = "black";
		cxt.lineCap = 'round';
		cxt.lineJoin = "round";
		cxt.stroke();

	}
	drawRight();

	document.getElementById('xiangmu').addEventListener('click', function() {
		window.location.assign('/xiangmupage');
	});
	document.getElementById('hometag').addEventListener('click', function() {
		window.location.assign('/');
	});
	document.getElementById('abouttag').addEventListener('click', function() {
		window.location.assign('/aboutpage');
	});
</script>


